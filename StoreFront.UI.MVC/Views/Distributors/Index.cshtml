@model IEnumerable<StoreFront.DATA.EF.Distributor>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.DIstributorName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CIty)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IsActive)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.State.StateName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.DIstributorName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CIty)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IsActive)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.State.StateName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.DIstributorsID }) |
            @Html.ActionLink("Details", "Details", new { id=item.DIstributorsID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.DIstributorsID })
        </td>
    </tr>
}

</table>

@model StoreFront.DATA.EF.Accessory

@{
    ViewBag.Title = "Add New Accessory";
}

<h2>@ViewBag.Title</h2>


@using (Html.BeginForm("Create", "Accessories", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true, "", new { @class = "text-danger" })


    <div id="aContainer">
        <div id="aHeader">


        </div>
        <div id="aLeft">
            @Html.LabelFor(model => model.AccessName, htmlAttributes: new { @class = "control-label" })
            @Html.EditorFor(model => model.AccessName, new { htmlAttributes = new { @class = "form-control" } })
            @Html.ValidationMessageFor(model => model.AccessName, "", new { @class = "text-danger" })
        </div>
        <div id="aRight">
            @Html.LabelFor(model => model.AccessDesc, htmlAttributes: new { @class = "control-label" })
            @Html.EditorFor(model => model.AccessDesc, new { htmlAttributes = new { @class = "form-control" } })
            @Html.ValidationMessageFor(model => model.AccessDesc, "", new { @class = "text-danger" })
        </div>
        <div class="clearfix"></div>

        <div id="bFooterLeft">
            @Html.ActionLink("Cancel", "Index", null, new { @class = "btn btn-danger" })
        </div>
        <div id="bFooterRight">
            <input type="submit" value="Add Accessory" class="btn btn-primary" />
        </div>
        <div class="clearfix"></div>
    </div>//closes container
}@*closes scope*@





















